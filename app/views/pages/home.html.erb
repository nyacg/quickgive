<h1>Pages#home</h1>
<p>Find me in app/views/pages/home.html.erb</p>

<% if authenticated? %>
  <p>Logged in as <%= current_user.email %>. <%= link_to "Log out", controller: :sessions, action: :destroy %></p>
<% else %>
  <p>Logged out.
  <%= form_tag(controller: "sessions", action: "create", method: "post") %> 
    <input name="email" type="text" />
    <%= submit_tag "Login" %>
  </form>
  </p>
<% end %>

<%= link_to "Example link to dashboard", controller: :pages, action: :dashboard %>
